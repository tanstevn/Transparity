{
  "profiles": {
    "Transparity-HTTP": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "scalar",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:3001"
    },
    "Transparity-HTTPS": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "scalar",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:3001;http://localhost:3000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/scalar",
      "publishAllPorts": true,
      "useSSL": true,
      "applicationUrl": "http://localhost:8443",
      "environmentVariables": {
        "ASPNETCORE_URLS": "http://+:8443"
      }
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}
